msgid ".iso"
msgstr ".iso"

#: C/brasero.xml:1103(filename)
msgid ".raw"
msgstr ".raw"

#: C/brasero.xml:1108(filename)
msgid ".cue"
msgstr ".cue"

#: C/brasero.xml:1113(filename)
